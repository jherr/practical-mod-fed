{
  "name": "non-federated",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "applications/*",
    "shared-ui/*",
    "logic/*",
    "services/*"
  ],
  "scripts": {
    "wipe": "rm -fr node_modules packages/**/node_modules",
    "build": "yarn workspaces run build",
    "start": "concurrently \"wsrun --parallel start\""
  },
  "dependencies": {
    "concurrently": "^5.2.0",
    "wsrun": "^5.2.1"
  }
}
